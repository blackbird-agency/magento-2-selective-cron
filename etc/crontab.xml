<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="blackbird_selective_cron_executor" instance="Blackbird\SelectiveCron\Cron\ExecuteSelectiveCron" method="execute">
            <schedule>* * * * *</schedule>
        </job>
        <job name="blackbird_selective_cron_scheduler" instance="Blackbird\SelectiveCron\Cron\ScheduleNewInstances" method="execute">
            <schedule>*/15 * * * *</schedule>
        </job>
    </group>
</config>